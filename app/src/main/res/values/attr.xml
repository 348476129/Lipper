<?xml version="1.0" encoding="utf-8"?>
<resources>


    <attr name="bubbleCount" format="integer" />
    <attr name="bubbleColor" format="color" />
    <declare-styleable name="BubbleView">
        <attr name="bubbleCount" />
        <attr name="bubbleColor" />
    </declare-styleable>


    <attr name="arc_height" format="dimension" />
    <attr name="arc_position">
        <enum name="bottom" value="0" />
        <enum name="top" value="1" />
        <enum name="left" value="2" />
        <enum name="right" value="3" />
    </attr>
    <attr name="arc_cropDirection" format="enum">
        <enum name="cropInside" value="0" />
        <enum name="cropOutside" value="1" />
    </attr>
    <declare-styleable name="ArcLayout">
        <attr name="arc_height" />
        <attr name="arc_position" />
        <attr name="arc_cropDirection"  />
    </declare-styleable>


</resources>